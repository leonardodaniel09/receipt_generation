<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>inicioUser</servlet-name>
        <servlet-class>servlets.inicioUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registroTra</servlet-name>
        <servlet-class>servlets.registroTra</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ventasUsuario</servlet-name>
        <servlet-class>servlets.ventasUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Modifi</servlet-name>
        <servlet-class>servlets.Modifi</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>inicioUser</servlet-name>
        <url-pattern>/iniciarU</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registroTra</servlet-name>
        <url-pattern>/registroT</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ventasUsuario</servlet-name>
        <url-pattern>/ventasU</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Modifi</servlet-name>
        <url-pattern>/Modifi</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
